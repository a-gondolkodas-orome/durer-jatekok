{
  "name": "durer-jatekok",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test:unit": "jest",
    "test": "npm run lint && npm run test:unit",
    "test:watch": "npm run test:unit -- --watchAll",
    "build": "parcel build src/index.html --public-url=/durer-jatekok/",
    "lint": "eslint 'src/**/*.js' --no-fix",
    "audit": "better-npm-audit audit -- --omit=dev",
    "dev": "parcel src/index.html --open --port=8012",
    "dev:nocache": "parcel serve src/index.html --no-cache --open --port=8012"
  },
  "dependencies": {
    "@headlessui/react": "1.7.15",
    "lodash": "4.17.21",
    "postcss": "8.4.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.1",
    "tailwindcss": "3.3.2",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.22.7",
    "@babel/preset-react": "7.22.5",
    "@testing-library/react": "14.0.0",
    "better-npm-audit": "3.7.3",
    "eslint": "8.44.0",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-react": "7.32.2",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "jest-extended": "4.0.0",
    "parcel": "2.9.3",
    "process": "0.11.10"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "postcss": {
    "plugins": {
      "tailwindcss": {}
    }
  }
}
